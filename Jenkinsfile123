pipeline {
    agent any
    stages {
        stage('repository checkout') {
            steps {
                git credentialsId: 'github', url: 'git@github.com:omeshvalyal/new_project.git'
            }
        }
stage('shell execution') {
            steps {
                sh 'cd /var/lib/jenkins/workspace/shell-pipeline'
                sh 'chmod 700 hello.sh'
                sh 'hello.sh'
            }
        }
    }
}
